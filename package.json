{
    "name": "@yookue/typedoc-plugin-raw-content",
    "version": "0.2.2",
    "title": "TypedocPluginRawContent",
    "description": "Generates extra raw html contents during typedoc compilation",
    "homepage": "https://yookue.github.io/typedoc-plugin-raw-content",
    "organization": "Yookue Ltd",
    "private": false,
    "license": "MIT",
