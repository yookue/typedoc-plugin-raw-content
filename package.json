        "typedoc",
        "typedoc-plugin",
        "typedoc-plugin-raw-content"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/cjs/index.js",
    "types": "dist/cjs/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "del-cli ./dist && tsc --outDir ./dist/cjs",
